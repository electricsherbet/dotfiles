        {
            "type": "player",
            "key": "│ ├󰥠",
            "keyColor": "green"
        },
        {
            "type": "media",
            "key": "│ └󰝚",
            "keyColor": "green"
        },
        {
            "type": "custom",
            "format": "└{$1}{$1}┘",
            "outputColor": "90"
        },
        "break",
        {
            "type": "custom",
            "format": " {#90}  {#31}  {#32}  {#33}  {#34}  {#35}  {#36}  {#37}  {#38}  {#39}       {#38}  {#37}  {#36}  {#35}  {#34}  {#33}  {#32}  {#31}  {#90}"
