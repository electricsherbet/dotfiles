            "key": "│ └",
            "keyColor": "red"
        },
        {
            "type": "wm",
            "key": " DE/WM",
            "keyColor": "magenta"
        },
        {
            "type": "lm",
            "key": "│ ├󰧨",
            "keyColor": "magenta"
        },
        {
            "type": "icons",
            "key": "│ ├󰀻",
            "keyColor": "magenta"
        },
        {
            "type": "terminal",
            "key": "│ ├",
            "keyColor": "magenta"
        },
        {
            "type": "terminalfont",
            "key": "│ ├󰬶",
            "keyColor": "magenta"
        },
        {
            "type": "cursor",
            "key": "│ ├",
            "keyColor": "magenta"
        },
        {
            "type": "font",
            "key": "│ ├󱀍",
            "keyColor": "magenta"
        },
        {
            "type": "wmtheme",
            "key": "│ ├󰉼",
            "keyColor": "magenta"
        },
        {
