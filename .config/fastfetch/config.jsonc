            "key": "│ └󰸉",
            "keyColor": "magenta"
        },
        {
            "type": "host",
            "key": "󰌢 PC",
            "keyColor": "cyan"
        },
        {
            "type": "cpu",
            "key": "│ ├󰻠",
            "keyColor": "cyan"
        },
        {
            "type": "gpu",
            "key": "│ ├󰍛",
            "keyColor": "cyan"
        },
        {
            "type": "disk",
            "key": "│ ├",
            "keyColor": "cyan"
        },
        {
            "type": "memory",
            "key": "│ ├󰑭",
            "keyColor": "cyan"
